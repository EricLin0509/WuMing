<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="wuming">
	<schema id="com.ericlin.wuming" path="/com/ericlin/wuming/">
	  	<key name="width" type="i">
			<default>400</default>
	  	</key>
	  	<key name="height" type="i">
			<default>600</default>
		</key>
		<key name="is-maximized" type="b">
			<default>false</default>
		</key>
		<key name="is-fullscreen" type="b">
	    	<default>false</default>
		</key>
		<key name="last-scan-time" type="s">
	    	<default>"1970.01.01 00:00:00"</default>
	  	</key>
		<key name="signature-expiration-time" type="i">
			<default>1</default>
		</key>
		<key name="enable-large-file" type="b">
			<default>false</default>
		</key>
		<key name="enable-pua" type="b">
			<default>false</default>
		</key>
		<key name="scan-archives" type="b">
			<default>false</default>
		</key>
		<key name="scan-mail" type="b">
			<default>false</default>
		</key>
		<key name="alert-exceeds-max" type="b">
			<default>false</default>
		</key>
		<key name="alert-encrypted" type="b">
			<default>false</default>
		</key>
	</schema>
</schemalist>
